bination als Set bereiten sie zudem als Geschenk Freude.<\/p>"}}},"block_order":["richtext_rFgj7x"],"name":"t:sections.rich_text.name","settings":{"color_scheme":"scheme-a66072b8-cdc0-4b4b-a3cc-bea4920e9b3b","content_width":"md","text_alignment":"left"}},"media_blocks_DbzGtA":{"type":"media-blocks","blocks":{"image_with_text_Tw3DQB":{"type":"image_with_text","settings":{"change_color_scheme_on_hover":false,"color_scheme":"","color_scheme_hover":"","image":"shopify:\/\/shop_images\/2023-11-17_Onomao0783.jpg","icon":"none","icon_width":32,"mobile_icon_width":24,"icon_position":"middle_center","subheading":"","subheading_position":"middle_center","title":"","heading_tag":"h4","heading_position":"middle_center","scrolling_title":false,"scrolling_title_separator":"none","show_text_vertically":false,"text":"","text_position":"middle_center","button_style":"solid","link_url":"shopify:\/\/collections\/vasen","link_text":"Vasen","link_position":"bottom_center"}},"image_with_text_GEg3Fm":{"type":"image_with_text","settings":{"change_color_scheme_on_hover":false,"color_scheme":"","color_scheme_hover":"","image":"shopify:\/\/shop_images\/2025-01-10_OnomaoStudio1231.jpg","icon":"none","icon_width":32,"mobile_icon_width":24,"icon_position":"middle_center","subheading":"","subheading_position":"middle_center","title":"","heading_tag":"h4","heading_position":"middle_center","scrolling_title":false,"scrolling_title_separator":"none","show_text_vertically":false,"text":"","text_position":"middle_center","button_style":"solid","link_url":"shopify:\/\/collections\/kerzen-kerzenstander","link_text":"Kerzen & Kerzenständer","link_position":"bottom_center"}},"image_with_text_M3AfLC":{"type":"image_with_text","settings":{"change_color_scheme_on_hover":false,"color_scheme":"scheme-3","color_scheme_hover":"scheme-3","image":"shopify:\/\/shop_images\/2023-08-30_Onomao4162_1.jpg","icon":"none","icon_width":32,"mobile_icon_width":24,"icon_position":"middle_center","subheading":"","subheading_po